{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "file-loader",
      {
        "name": "[hash].[ext]",
        "extensions": [
          "png",
          "jpg",
          "jpeg",
          "gif",
          "svg"
        ],
        "publicPath": "/static",
        "outputPath": "/dist/static",
        "context": "/src",
        "limit": 0
      }
    ]
  ]
}